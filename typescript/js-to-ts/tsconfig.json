{
  "compilerOptions": {
    "outDir": "js",
    "module": "ES6",
    "target": "ES6",
    "types": [],

    "sourceMap": true,
    "noUncheckedIndexedAccess": true,
    "exactOptionalPropertyTypes": true,
    "strict": true,
    "noUncheckedSideEffectImports": true,
    "skipLibCheck": true
  },
  "include": ["**/*.ts"]
}
